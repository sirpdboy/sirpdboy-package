#~ msgstr "新目录权限掩码"

#~ msgid "Mask for new files"
#~ msgstr "新文件权限掩码"

#~ msgid "Physical Path"
#~ msgstr "物理路径"

msgid "Auto Share"
msgstr "自动共享"

msgid "Auto share local disk which connected"
msgstr "开机自动匿名共享路由器所连接的硬盘"

